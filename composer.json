{
    "name": "netflex/structures",
    "description": "Netflex Structure library",
    "type": "library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Netflex\\Structure\\": ""
        }
    },
    "replace": {
        "netflex/structure": "*"
    },
    "require": {
        "php": "^8.4",
        "apility/rule-builder": "^1.5.2",
        "haydenpierce/class-finder": "^0.4.3",
        "netflex/pages": "^6.0",
        "netflex/query-builder": "^6.0",
        "netflex/support": "^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Netflex\\Structure\\Providers\\StructureServiceProvider"
            ]
        }
    }
}
